{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "indent": 0,
        "max-len": 0,
        "padded-blocks": 0,
        "lines-between-class-members": 0,
        "no-trailing-spaces": 0,
        "new-cap": 0,
        "bject-curly-newline": 0
    }
}